<odoo>
    <data>
        <!-- List View -->
        <record model="ir.ui.view" id="master_plan_tree">
            <field name="name">master.plan.tree</field>
            <field name="model">master.plan</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name" />
                </tree>
            </field>
        </record>

        <!-- Form View -->
        <record model="ir.ui.view" id="master_plan_form">
            <field name="name">master.plan.form</field>
            <field name="model">master.plan</field>
            <field name="arch" type="xml">
                <form>
                    <header></header>
                    <sheet>
                        <group>
                            <group>
                                <field name="name" />
                            </group>
                            <group></group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.actions.act_window" id="master_plan_action">
            <field name="name">Nama Model</field>
            <field name="res_model">master.plan</field>
            <field name="view_mode">tree,form</field>
        </record>

    </data>
</odoo>