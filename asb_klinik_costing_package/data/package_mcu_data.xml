<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
    
        <!-- Sequence -->

        <record id="seq_package_mcu_house" model="ir.sequence">
            <field name="name">Package MCU in House</field>
            <field name="code">package.mcu.house</field>
            <field name="implementation">no_gap</field>
            <field name="prefix">MKT/BOP/%(month)s.%(y)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_package_mcu_onsite" model="ir.sequence">
            <field name="name">Package MCU Onsite</field>
            <field name="code">package.mcu.onsite</field>
            <field name="implementation">no_gap</field>
            <field name="prefix">MKT/BOP/%(month)s.%(y)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    
    </data>
</odoo>
