<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record model="ir.ui.view" id="preview_list_package_house_kanban_view">
            <field name="name">preview.list.package.house.kanban</field>
            <field name="model">list.package</field>
            <field name="arch" type="xml">
                <kanban class="o_kanban_small_column o_opportunity_kanban" create="false" edit="false" delete="false">
                    <field name="currency_id" invisible="1"/>
                    <field name="name"/>
                    <field name="nama_paket"/>
                    <field name="package_mcu_id"/>
                    <field name="perusahaan_id"/>
                    <field name="certificate_ids" />
                    <field name="bahasa_hasil" />
                    <field name="price_paket" />
                    <templates>
                        <t t-name="kanban-box">
                            <div class="oe_kanban_global_click">
                                <div class="oe_kanban_content">
                                    <div class="o_kanban_record_title">
                                        <strong>
                                            <field name="perusahaan_id"/>
                                        </strong>
                                    </div>
                                    <div class="o_kanban_record_subtitle">
                                        <t t-if="record.name.raw_value">
                                            <strong>
                                                <field name="name"/>
                                            </strong>
                                        </t>
                                    </div>
                                    <div class="o_kanban_record_subtitle">
                                        <t t-if="record.nama_paket.raw_value">
                                            <strong>
                                                <field name="nama_paket"/>
                                            </strong>
                                        </t>
                                    </div>
                                    <div class="o_kanban_record_subtitle">
                                        <field name="certificate_ids" widget="many2many_tags" options="{'color_field': 'color'}"/>
                                    </div>
                                    <div class="o_kanban_record_bottom">
                                        <div class="oe_kanban_bottom_left">
                                            <field name="bahasa_hasil"/>
                                        </div>
                                        <div class="oe_kanban_bottom_right">
                                            <field name="price_paket" widget="monetary" options="{'currency_field': 'currency_id'}"/>
                                        </div>
                                    </div>
                                </div>
                                <div class="oe_clear"/>
                            </div>
                        </t>
                    </templates>
                </kanban>
            </field>
        </record>

        <record id="preview_list_package_house_view_form" model="ir.ui.view">
            <field name="name">preview.list.package.house.view.form</field>
            <field name="model">list.package</field>
            <field name="arch" type="xml">
                <form string="List Package" copy="false" create="false" edit="false" delete="false">

                    <header>
                        <field name="list_package_ids" invisible="1"/>
                    </header>

                    <sheet style="max-width:40%;">
                        <div class="oe_title">
                            <h1>
                                <field name="perusahaan_id"/>
                            </h1>
                        </div>

                        <div class="oe_title">
                            <label for="nama_paket" string="Nama Paket"/>
                            <h2>
                                <field name="nama_paket"/>
                            </h2>
                        </div>

                        <group style="margin: 1px 0px;">
                            <group style="margin: 1px 0px;">
                                <field name="package_mcu_id" force_save="1" invisible="1"/>
                                <field name="bahasa_hasil" force_save="1" invisible="1"/>
                                <field name="in_house" force_save="1" invisible="1"/>
                                <field name="onsite" force_save="1" invisible="1"/>
                                <field name="currency_id" force_save="1" invisible="1"/>
                                <field name="name" />
                                <field name="certificate_ids" widget="many2many_tags" options="{'no_open': True, 'no_create': True, 'no_create_edit':True, 'no_quick_create': True, 'color_field': 'color'}"/>
                            </group>

                            <group style="margin: 1px 0px;">
                                <field name="option" widget="boolean_toggle" />
                                <field name="data_perusahaan_id" attrs="{ 'invisible' : [('option', '=', False)]}" options="{'no_open': True, 'no_create': True, 'no_create_edit':True, 'no_quick_create': True}"/>
                                <field name="list_package_id" domain="[('id', 'in', list_package_ids)]" attrs="{ 'invisible' : ['|', ('data_perusahaan_id', '=', False), ('option', '=', False)]}" options="{'no_open': True, 'no_create': True, 'no_create_edit':True, 'no_quick_create': True}"/>
                            </group>
                        </group>

                        <notebook>
                            <page name="master_list_package" string="Daftar Package">
                                <field name="examination_list_ids" widget="many2many" options="{'no_create': True}" force_save="1" context="{'tree_view_ref': 'asb_klinik_costing_package.config_examination_list_view_tree'}">
                                    <tree>
                                        <field name="name" string="Examination List"/>
                                        <field name="poli_unit_id" />
                                        <field name="total_beban_langsung" string="Cost" widget="monetary"/>
                                        <field name="master_tindakan_id" invisible="1"/>
                                        <field name="list_price" string="Harga Jual" widget="monetary" invisible="1"/>
                                    </tree>
                                    <form string="Examination List">
                                        <sheet>
                                            <group>
                                                <field name="name" />
                                                <field name="poli_unit_id"/>
                                                <field name="total_beban_langsung" string="Cost" widget="monetary"/>
                                            </group>
                                        </sheet>
                                    </form>
                                </field>

                                <field name="sampling_list_ids" widget="many2many" readonly="1" force_save="1" options="{'no_open': True, 'no_create': True, 'no_create_edit':True, 'no_quick_create': True}">
                                    <tree>
                                        <field name="name" string="Sampling"/>
                                        <field name="total_cost" string="Cost" widget="monetary"/>
                                    </tree>
                                </field>

                                <field name="fixed_costing_line" readonly="1" force_save="1" options="{'no_open': True, 'no_create': True, 'no_create_edit':True, 'no_quick_create': True}">
                                    <tree>
                                        <field name="fixed_costing_id"/>
                                        <field name="name" invisible = "1"/>
                                        <field name="cost_in_house" widget="monetary"/>
                                    </tree>
                                    <form string="Fixed Costing" readonly="1" force_save="1">
                                        <sheet>
                                            <group col="4" colspan="2">
                                                <field name="fixed_costing_id" />
                                                <field name="cost_in_house" widget="monetary"/>
                                            </group>
                                        </sheet>
                                    </form>
                                </field>

                                <div class="row">
                                    <div class="col-md-9 text-right">
                                        <label for="total_cost_examination_list_id" class="o_light_label">Total Cost Examination List (IDR)</label>
                                    </div>

                                    <div class="col-md-2 text-right">
                                        <field name="total_cost_examination_list" widget="monetary" id="total_cost_examination_list_id" class="oe_inline"/>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-md-9 text-right">
                                        <label for="total_cost_sampling_id" class="o_light_label">Total Cost Sampling (IDR)</label>
                                    </div>

                                    <div class="col-md-2 text-right">
                                        <field name="total_cost_sampling" widget="monetary" id="total_cost_sampling_id" class="oe_inline"/>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-md-9 text-right">
                                        <label for="total_cost_certificate_id" class="o_light_label">Total Cost Certificate (IDR)</label>
                                    </div>

                                    <div class="col-md-2 text-right">
                                        <field name="total_cost_certificate" widget="monetary" id="total_cost_certificate_id" class="oe_inline"/>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-md-9 text-right">
                                        <label for="total_cost_fixed_costing_id" class="o_light_label">Total Cost Fixed Costing (IDR)</label>
                                    </div>

                                    <div class="col-md-2 text-right">
                                        <field name="total_cost_fixed_costing" widget="monetary" id="total_cost_fixed_costing_id" class="oe_inline"/>
                                    </div>
                                </div>

                                <div class="row">
                                    <hr style="width:50%;text-align:left;margin-left:auto;margin-right:0"/>
                                </div>

                                <div class="row">
                                    <div class="col-md-7 text-right">
                                        <label for="direct_cost_id" class="o_light_label">Direct Cost Allocation</label>
                                    </div>

                                    <div class="col-md-2 text-right">
                                        <field name="direct_cost" id="direct_cost_id"/>
                                        <span class="oe_inline">%</span>
                                    </div>

                                    <div class="col-md-2 text-right">
                                        <field name="total_direct_cost" widget="monetary" class="oe_inline"/>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-md-7 text-right">
                                        <label for="fixed_cost_id" class="o_light_label">Direct Cost Allocation</label>
                                    </div>

                                    <div class="col-md-2 text-right">
                                        <field name="fixed_cost" id="fixed_cost_id"/>
                                        <span class="oe_inline">%</span>
                                    </div>

                                    <div class="col-md-2 text-right">
                                        <field name="total_fixed_cost" widget="monetary" class="oe_inline"/>
                                    </div>
                                </div>

                                <div class="row">
                                    <hr style="width:50%;text-align:left;margin-left:auto;margin-right:0"/>
                                </div>

                                <div class="row">
                                    <div class="col-md-8 text-right">
                                        <h3>Total Cost (IDR)</h3>
                                    </div>

                                    <div class="col-md-2 text-right">
                                        <h3>
                                            <field name="total_cost" widget="monetary" class="oe_inline"/>
                                        </h3>
                                    </div>
                                </div>

                                <div class="row">
                                    <hr style="width:50%;text-align:left;margin-left:auto;margin-right:0"/>
                                </div>

                                <div class="row">
                                    <div class="col-md-6 text-right">
                                        <label for="profit_id" class="o_light_label">Profit</label>
                                    </div>

                                    <div class="col-md-2 text-right">
                                        <field name="profit" id="profit_id"/>
                                    </div>

                                    <div class="col-md-1">
                                        <span class="oe_inline">%</span>
                                    </div>

                                    <div class="col-md-2 text-right">
                                        <field name="profit_rupiah" widget="monetary" nolabel="1" class="oe_inline"/>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-md-9 text-right">
                                        <label for="manual_fee_id" class="o_light_label">Manual Fee (IDR)</label>
                                    </div>

                                    <div class="col-md-2 text-right">
                                        <field name="manual_fee" widget="monetary" id="manual_fee_id" class="oe_inline"/>
                                    </div>
                                </div>

                                <div class="row">
                                    <hr style="width:50%;text-align:left;margin-left:auto;margin-right:0"/>
                                </div>

                                <div class="row">
                                    <div class="col-md-8 text-right">
                                        <h1>Price (IDR)</h1>
                                    </div>

                                    <div class="col-md-2 text-right">
                                        <h1>
                                            <field name="price_paket" widget="monetary" class="oe_inline"/>
                                        </h1>
                                    </div>
                                </div>
                            </page>
                        </notebook>
                    </sheet>

                    <div class="oe_chatter">
                        <field name="message_follower_ids" widget="mail_followers"/>
                        <field name="message_ids" widget="mail_thread"/>
                    </div>

                </form>
            </field>
        </record>

        <record id="preview_list_package_house_view_search" model="ir.ui.view">
            <field name="name">preview.list.package.house.view.search</field>
            <field name="model">list.package</field>
            <field name="arch" type="xml">

                <search string="List Package">
                    <field name="package_mcu_id" filter_domain="[('package_mcu_id', 'child_of', raw_value)]"/>
                    <separator/>
                </search>

            </field>
        </record>

