<?xml version="1.0" encoding="UTF-8"?>

<templates>
    <t t-name="asb_helpline_call_record.CallRecordDashboard">
        <div class="o_call_record_dashboard container">
            <div class="row">
                <div class="col-sm-12">
                    <table class="table table-borderless">
                        <tbody>
                            <th colspan="5" style="text-align: center">SOURCE</th>
                            <th colspan="1" style="text-align: center"></th>
                            <th colspan="2" style="text-align: center">STATUS</th>
                            <tr>
                                <td class="o_main_call o_dashboard_action" title="Call In" name="asb_helpline_call_record.call_record_dashboard" context='{"search_default_ticketing_call_in": 1}' style="box-shadow: 0 2px 5px 0 rgb(0 0 0 / 50%), 0 2px 10px 0 rgb(0 0 0 / 12%);">
                                    <table style="width:100%">
                                        <tr>
                                            <td>
                                                <img src='/asb_helpline_call_record/static/src/img/call.png' style="max-width: 100%;" />
                                            </td>

                                            <td class="o_remarks">
                                                <span>Call In</span>
                                                <br />
                                                <span>
                                                    <t t-esc="values['total_caller_in']" />
                                                </span>
                                            </td>
                                        </tr>
                                    </table>
                                </td>
                                <td class="o_main_call o_dashboard_action" title="Email In" name="asb_helpline_call_record.call_record_dashboard" context='{"search_default_ticketing_email_in": 1}' style="box-shadow: 0 2px 5px 0 rgb(0 0 0 / 50%), 0 2px 10px 0 rgb(0 0 0 / 12%);">
                                    <table style="width:100%">
                                        <tr>
                                            <td>
                                                <img src='/asb_helpline_call_record/static/src/img/email.png' style="max-width: 100%;" />
                                            </td>

                                            <td class="o_remarks">
                                                <span>Email In</span>
                                                <br />
                                                <span>
                                                    <t t-esc="values['total_email_in']" />
                                                </span>
                                            </td>
                                        </tr>
                                    </table>
                                </td>
                                <td class="o_main_call o_dashboard_action" title="Whatsapp" name="asb_helpline_call_record.call_record_dashboard" context='{"search_default_ticketing_whatsapp": 1}' style="box-shadow: 0 2px 5px 0 rgb(0 0 0 / 50%), 0 2px 10px 0 rgb(0 0 0 / 12%);">
                                    <table style="width:100%">
                                        <tr>
                                            <td>
                                                <img src='/asb_helpline_call_record/static/src/img/whatsapp.png' style="max-width: 100%;" />
                                            </td>

                                            <td class="o_remarks_whatsapp">
                                                <span>Whatsapp</span>
                                                <br />
                                                <span>
                                                    <t t-esc="values['total_whatsapp']" />
                                                </span>
                                            </td>
                                        </tr>
                                    </table>
                                </td>
                                <td class="o_main_call o_dashboard_action" title="E-Client" name="asb_helpline_call_record.call_record_dashboard" context='{"search_default_ticketing_eclient": 1}' style="box-shadow: 0 2px 5px 0 rgb(0 0 0 / 50%), 0 2px 10px 0 rgb(0 0 0 / 12%);">
                                    <table style="width:100%">
                                        <tr>
                                            <td>
                                                <img src='/asb_helpline_call_record/static/src/img/eclient.png' style="max-width: 100%;" />
                                            </td>

                                            <td class="o_remarks">
                                                <span>E-Client</span>
                                                <br />
                                                <span>
                                                    <t t-esc="values['total_eclient']" />
                                                </span>
                                            </td>
                                        </tr>
                                    </table>
                                </td>
                                <td class="o_main_call o_dashboard_action" title="Others" name="asb_helpline_call_record.call_record_dashboard" context='{"search_default_ticketing_others": 1}' style="box-shadow: 0 2px 5px 0 rgb(0 0 0 / 50%), 0 2px 10px 0 rgb(0 0 0 / 12%);">
                                    <table style="width:100%">
                                        <tr>
                                            <td>
                                                <img src='/asb_helpline_call_record/static/src/img/others.png' style="max-width: 100%;" />
                                            </td>

                                            <td class="o_remarks">
                                                <span>Others</span>
                                                <br />
                                                <span>
                                                    <t t-esc="values['total_others']" />
                                                </span>
                                            </td>
                                        </tr>
                                    </table>
                                </td>
                                <td class="o_text">
                                    <div></div>
                                </td>
                                <td class="o_main_call o_dashboard_action" title="On Process" name="asb_helpline_call_record.call_record_dashboard" context='{"search_default_state_open": 1}' style="box-shadow: 0 2px 5px 0 rgb(0 0 0 / 50%), 0 2px 10px 0 rgb(0 0 0 / 12%);">
                                    <table style="width:100%">
                                        <tr>
                                            <td>
                                                <img src='/asb_helpline_call_record/static/src/img/open.png' style="max-width: 100%;" />
                                            </td>

                                            <td class="o_remarks">
                                                <span>Open</span>
                                                <br />
                                                <span>
                                                    <t t-esc="values['total_open']" />
                                                </span>
                                            </td>
                                        </tr>
                                    </table>
                                </td>
                                <td class="o_main_call o_dashboard_action" title="Closed" name="asb_helpline_call_record.call_record_dashboard" context='{"search_default_state_close": 1}' style="box-shadow: 0 2px 5px 0 rgb(0 0 0 / 50%), 0 2px 10px 0 rgb(0 0 0 / 12%);">
                                    <table style="width:100%">
                                        <tr>
                                            <td>
                                                <img src='/asb_helpline_call_record/static/src/img/closed.png' style="max-width: 100%;" />
                                            </td>

                                            <td class="o_remarks">
                                                <span>Closed</span>
                                                <br />
                                                <span>
                                                    <t t-esc="values['total_close']" />
                                                </span>
                                            </td>
                                        </tr>
                                    </table>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </t>
</templates>